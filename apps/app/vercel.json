{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "ignoreCommand": "node scripts/skip-ci.js",
  "installCommand": "corepack enable && corepack prepare pnpm@latest --activate && cd ../.. && pnpm install",
  "buildCommand": "cd ../.. && pnpm build --filter=app",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  }
}
